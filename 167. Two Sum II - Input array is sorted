class Solution: #36 ms
    def twoSum(self, numbers, target):
        """
        :type numbers: List[int]
        :type target: int
        :rtype: List[int]
        """
        dict = {}
        for i in range(len(numbers)):
            if numbers[i] in dict:
                return [dict[numbers[i]]+1,i+1]
            else:
                dict[target-numbers[i]] = i 
        
